{
  "name": "radiovis-stomp-server",
  "version": "0.0.9",
  "description": "STOMP server implementation for RadioVis",
  "main": "index.js",
  "bin": "./bin/radiovis-stomp-server",
  "scripts": {
    "start": "node ./bin/radiovis-stomp-server",
